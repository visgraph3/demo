<?xml version="1.0"?>
<sparql xmlns="http://www.w3.org/2005/sparql-results#">

  <head>
    <variable name="name"/>
    <variable name="age"/>
  </head>

  <results ordered="true" distinct="false">
    <result> 
      <binding name="name">
		<literal xml:lang="en">Bob</literal>
      </binding>
	<binding name="age">
		<literal datatype="http://www.w3.org/2001/XMLSchema#integer">30</literal>
      </binding>

    </result>
	
  </results>
  

</sparql>